

@{
    ViewData["Title"] = "Company";
}

<!-- Content Wrapper. Contains page content -->
<div class="content-wrapper">

    <!-- Main content -->
    <section class="content">
        <!-- Default box -->
        <div class="box">
            <div class="box-header with-border">
                <h3 class="box-title text-primary"><i class="fa fa-users"></i> @ViewData["Title"]</h3>
                <div class="box-tools pull-right">
                    <button type="button" class="btn btn-box-tool" data-widget="collapse" data-toggle="tooltip"
                            title="Collapse">
                        <i class="fa fa-minus"></i>
                    </button>
                    <button type="button" class="btn btn-box-tool" data-widget="remove" data-toggle="tooltip" title="Remove">
                        <i class="fa fa-times"></i>
                    </button>
                </div>
            </div>
            <div class="box-body">
            <div class="row">
                 <div class="col-sm-6" style="padding-left: 25px;margin-top: 10px;">
                    <div class="mb-md">
                    <a class="btn btn-success" id="Addnew" style="margin-bottom:10px"><i class="fa fa-plus"></i> Add New</a>
                    @*<button id="Delect" type="button" class="mb-xs mt-xs mr-xs btn btn-danger" onclick="return testCheck();">Delect</button>*@
                     </div>
                </div>
            </div>  
            
              <table id="tablecompany" class="display" style="width:100%;"></table>
               @{await Html.RenderPartialAsync("_ModalPartial");}
            <!-- /.box-footer-->
             </div>
        <!-- /.box -->
    </section>
    <!-- /.content -->
</div>
<!-- /.content-wrapper -->
@section Scripts{
<script src="~/scripts/Kisugar/Company.js"></script>

}

